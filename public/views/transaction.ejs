<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Node Typescript</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
    integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
    integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous">
  </script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
    integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous">
  </script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
    integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous">
  </script>
</head>

<body>
  <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th class="th-sm">No.

        </th>
        <th class="th-sm">Supplier Name

        </th>
        <th class="th-sm">Amount

        </th>
        <th class="th-sm">Advanced Value

        </th>
        <th class="th-sm">Order Value

        </th>
        <th class="th-sm">Payment Fee

        </th>
        <th class="th-sm">Platform Fee

        </th>

        <th class="th-sm">Status

        </th>

        <th class="th-sm">Description

        </th>

        <th class="th-sm">Action

        </th>
      </tr>
    </thead>
    <tbody>
      <% transactions.forEach((element, index) => {%>
      <tr>
        <td><%= index%></td>
        <td><%= element.name %></td>
        <td><%= element.amount %></td>
        <td><%= element.advancefee %></td>
        <td><%= element.ordervalue %></td>
        <td><%= element.paymentfee %></td>
        <td><%= element.platformfee %></td>
        <td><%= element.status %></td>
        <td><%= element.description %></td>
        <td><a href='<%= element.paymentlink%>'><button class="btn btn"> Payment </button></a></td>
      </tr>
      <%}) %>
    </tbody>
  </table>
</body>

</html>